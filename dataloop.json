{
    "displayName": "Getty Images",
    "name": "getty-images",
    "description": "Add image to a dataset based on Getty Images database.",
    "scope": "public",
    "version": "0.0.14",
    "attributes": {
        "Provider": "Getty"
    },
    "codebase": {
        "type": "git",
        "gitUrl": "https://github.com/dataloop-ai-apps/getty-images.git",
        "gitTag": "0.0.14"
    },
    "components": {
        "toolbars": [
            {
                "displayName": "Import Getty Images",
                "invoke": {
                    "type": "function",
                    "namespace": "getty_images.search_and_add_images"
                },
                "location": "datasetBrowserApps",
                "icon": "icon-dl-edit"
            }
        ],
        "modules": [
            {
                "name": "getty_images",
                "entryPoint": "runner.py",
                "className": "ServiceRunner",
                "initInputs": [],
                "functions": [
                    {
                        "name": "search_and_add_images",
                        "displayName": "Import Getty Images",
                        "description": "Add images to the current Dataset from the Getty Images search",
                        "input": [
                            {
                                "type": "Dataset",
                                "name": "dataset",
                                "description": "Destination Dataset"

                            },
                            {
                                "type": "String",
                                "name": "phrase",
                                "description": "Search phrase for the Getty platform"
                            },
                            {
                                "type": "Int",
                                "name": "n_images",
                                "description": "Number of images to add"
                            }
                        ],
                        "output": [
                        ],
                        "displayIcon": "crop"
                    }
                ]
            }
        ],
        "services": [
            {
                "name": "getty-images",
                "moduleName": "getty-images",
                "initParams": {},
                "secrets": [],
                "runtime": {
                    "podType": "regular-xs",
                    "runnerImage": "gcr.io/viewo-g/piper/agent/runner/cpu/getty_images:0.5.0",
                    "numReplicas": 1,
                    "concurrency": 10,
                    "singleAgent": false,
                    "autoscaler": {
                        "type": "rabbitmq",
                        "minReplicas": 0,
                        "maxReplicas": 1,
                        "queueLength": 10
                    },
                    "preemptible": false
                },
                "operation": "none"
            }
        ]
    }
}
